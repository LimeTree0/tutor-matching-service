<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tutormatching.dotommorow.repository.mapper.TestMapper">

    <insert id="save">
        insert into test (name)
        values (#{name})
    </insert>

    <update id="update">
        update test
        set name = #{name}
        where id = #{id}
    </update>

    <select id="findById" resultType="String">
        select *
        from test
        where id = #{id}
    </select>

    <select id="findAll" resultType="String">
        select *
        from test;
    </select>

</mapper>